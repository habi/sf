Options +FollowSymLinks
RewriteEngine On
RewriteBase /


# add .php file extension
RewriteRule ^nuclear-myth.html$ internet-history/ [L]
RewriteRule ^nuclear-myth$ internet-history/ [L]
RewriteRule ^internet-history$ internet-history/ [L]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.+)$ $1.php [L]
